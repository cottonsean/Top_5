<h1>Rock#roll</h1>
<p>Find me in app/views/rock/roll.html.erb</p>
<div class="row">
  <h2> My Top 5 </h2>
    <% @songs.each do |song| %>
      <div class="well well-lg">
        <h2><%= song.name %></h2>
	<h3>by <%= song.artist.name %> </h3>
	<p><% "#{song.rating}".to_i.times do |rank| %>
	  <span class="glyphicon glyphicon-star"></span>
        <% end %>
        </p>	             
     </div>
  <% end %>
</div>